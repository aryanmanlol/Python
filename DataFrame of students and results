import pandas as pd
student={'Name':['SWARAJ','VARUN','NIKHIL','DHANAY'],
          'Dept.':['IT','CS','CS','EXTC'],
          'Score':[2.5,4.0,2.7,3.8]}
i=[1,2,3,4]
df=pd.DataFrame(student,i)
print(df)

print("score greater than 3")
print(df,iloc[[1]])
print(df.iloc[[3]])

print("first two records")
print(df.head(2))

print('Score less than 3')
print(df.iloc[[0]])
print(df.iloc[[1]])
