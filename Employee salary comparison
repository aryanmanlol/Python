class employee:
  def __init__(self,name,salary):
    self.name=name
    self.salary=salary
e1=employee('messi',12000)
e2=employee('ronaldo',1500)
if e1.salary>e2.salary:
  print(" messi has more salary")
elif e1.salary<e2.salary:
  print(" ronaldo has more salary")
else:
  print("both have equal salary")
